// Test301

program test301

	const int nula = 0;
	const int jedan = 1;
	const int pet = 5;

	int niz[], niz2[], a, b,c,d;
	char nizch[];
{
	void main()	
		int bodovi;
		bool bt;
	{
		niz = new int[3];
		niz[nula] = jedan; // niz[0] = 1
		niz[1] = 2; 
		niz[niz[jedan]] = niz[niz[0]] * 3; // niz[2] = niz[1] * 3 = 2 * 3 = 6
		bodovi = niz[2]/niz[0]; // bodovi = 6 / 1 = 6
		print(bodovi); // print(bodovi) => 6
		print(niz[2]); // print(niz[2]) => 6
		
		
		[a,b] = niz;
		print(a);	// niz[0] -> 1
		print(b);	// niz[1] -> 2
		
		
		[,a,] = niz;
		print(a);	// niz[1] -> 2

		niz2 = new int[2];
		[niz2[0],,niz2[niz[nula]]] = niz;

		print(niz2[0]);	// niz[0] -> 1
		print(niz2[1]); // niz[2] -> 6 
	}
}	